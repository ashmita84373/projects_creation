username=("ashmita","sanjana","harman")
password=("ashm","ghij","uyhj")
username_1=input("enter your username")
password_1=input("enter your password")
authenticated=False
for i in range(3):
    
    if username_1==username[i] and password_1==password[i]:
       authenticated=True
       break

if authenticated:
    print("login successfully")
else:
    print("login failed")
